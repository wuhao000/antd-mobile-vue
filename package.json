{
  "name": "antd-mobile-vue",
  "version": "0.4.2-1",
  "description": "基于Vue和ant-desing的及PC端综合组件库",
  "main": "es/index.js",
  "keyword": "antd antd-mobile vue",
  "author": {
    "email": "wh1200@126.com",
    "name": "wuhao"
  },
  "repository": {
    "type": "github",
    "url": "https://github.com/wuhao000/antd-mobile-vue"
  },
  "license": "Apache-2.0",
  "directories": {
    "lib": "lib"
  },
  "private": false,
  "scripts": {
    "create": "tsc -p build && node build/create.js && npm run docs",
    "docs": "tsc -p build && node build/check.js && node build/def.js && node build/docs.js",
    "dev": "tsc -p build && npm run docs && vue-cli-service serve",
    "build": "tsc -p build && npm run docs && node compile/build-components.js && vue-cli-service build",
    "lint": "vue-cli-service lint",
    "lib": "npm run docs && vue-cli-service build --target lib --name antdm --dest lib src/packages/index.ts",
    "remove": "tsc -p build && node build/remove.js && npm run docs",
    "publish": "npm run docs",
    "cdn": "tsc -p build && npm run lib && node build/cdn.js",
    "build:components": "npm run docs && node compile/build-components.js"
  },
  "files": [
    "dist",
    "packages",
    "src",
    "README.md",
    "types",
    "es"
  ],
  "publishConfig": {
    "registry": "http://registry.npmjs.org"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "dependencies": {
